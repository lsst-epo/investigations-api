{
  "require": {
    "castiron/next-builds": "^1.0",
    "craftcms/cms": "4.7.1",
    "craftcms/google-cloud": "2.0.0",
    "craftcms/redactor": "3.0.4",
    "jamesedmonston/graphql-authentication": "2.5.0",
    "lsst-epo/canto-dam-assets": "4.0.12",
    "sebastianlenz/linkfield": "^2.1.4",
    "spicyweb/craft-neo": "4.0.3",
    "verbb/super-table": "^3.0",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/lsst-epo/canto-dam-assets.git"
    },
    {
      "type": "path",
      "url": "plugins/nextbuilds"
    },
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ],
  "require-dev": {
    "craftcms/generator": "^1.4",
    "yiisoft/yii2-shell": "^2.0.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\userregistrationmodule\\": "modules/userregistrationmodule/src/",
      "modules\\investigations\\": "modules/investigations/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.1.10"
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
